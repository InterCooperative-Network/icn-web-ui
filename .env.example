# ICN Web UI Environment Configuration

# ICN Node API Configuration
VITE_ICN_API_URL=http://localhost:8080/api/v1
VITE_ICN_WS_URL=ws://localhost:8080/ws

# Network Configuration
VITE_ICN_NETWORK_NAME=development
VITE_ICN_NETWORK_ID=dev-001

# Application Configuration
VITE_APP_NAME=ICN Web UI
VITE_APP_VERSION=0.1.0
VITE_APP_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_GOVERNANCE=true
VITE_ENABLE_MESH_JOBS=true
VITE_ENABLE_MANA_TRANSFERS=true
VITE_ENABLE_NETWORK_MONITORING=true

# Development/Debug
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info
